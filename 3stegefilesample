pipeline {
    agent {lubuntu-01-r}
    stages {
        stage('check') {
            steps {
                mkdir check
                touch testfile
            }
        }
        stage('Test') {
            steps {
                cat testfile
            }
        }
        stage('deploy') {
            steps {
                echo "this is a declarative jenkinsfile testfile" >> testfile
             }
        }
        stage ('build') {
            steps {
                display
            }
        }
    }
}
